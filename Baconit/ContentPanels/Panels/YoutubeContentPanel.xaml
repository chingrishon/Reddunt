<UserControl x:Class="Baconit.ContentPanels.Panels.YoutubeContentPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="using:Baconit.ContentPanels.Panels"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:rs4="http://schemas.microsoft.com/winfx/2006/xaml/presentation?IsApiContractPresent(Windows.Foundation.UniversalApiContract, 6)"
             d:DesignHeight="300"
             d:DesignWidth="400"
             mc:Ignorable="d">

    <Grid x:Name="ui_contentRoot" Background="Black" >
        <MediaElement x:Name="m_youTubeVideo" AreTransportControlsEnabled="True" >
            <MediaElement.TransportControls>
                <MediaTransportControls IsCompact="True" rs4:IsCompactOverlayEnabled="True" rs4:IsCompactOverlayButtonVisible="True">
                   

                </MediaTransportControls>
            </MediaElement.TransportControls>
        </MediaElement>
    </Grid>


</UserControl>